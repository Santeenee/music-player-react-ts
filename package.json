{
	"name": "musicplayer",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"watch-tl": "pnpm exec tailwindcss -i ./public/assets/input.css -o ./dist/assets/build.css --watch",
		"build-tl": "pnpm exec tailwindcss -i ./public/assets/input.css -o ./dist/assets/build.css --minify",
		"build": "pnpm build-tl && tsc && vite build",
		"preview": "vite preview",
		"both": "pnpm build && pnpm preview"
	},
	"dependencies": {
		"classnames": "^2.3.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.24",
		"@types/react-dom": "^18.0.8",
		"@vitejs/plugin-react": "^2.2.0",
		"prettier": "^2.8.3",
		"prettier-plugin-tailwindcss": "^0.2.2",
		"tailwindcss": "^3.2.4",
		"typescript": "^4.6.4",
		"vite": "^3.2.3"
	}
}
